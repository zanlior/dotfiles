{
    "position": "top",
    "modules-left": [
        "custom/launcher",
        "mpris"
    ],
    "modules-center": [
        "clock",
        "hyprland/workspaces"
    ],
    "modules-right": [
        "pulseaudio",
        "network",
        "swaync"
    ],
    "mpris": {
        "format": "{player_icon} {title}",
        "format-paused": "{status_icon} <i>{position} {artist}</i>",
        "player-icons": {
            "default": "",
            "firefox": "",
            "mpv": "",
            "spotify": ""
        },
        "status-icons": {
            "paused": ""
        },
        "max-length": 30
    },
    "clock": {
        "format": "{:%I:%M %p}",
        "format-alt": "{:%a, %b %d}"
    },
    "hyprland/workspaces": {
        "format": ""
    },
    "persistent-workspaces": 5,
    "pulseaudio": {
        "format": "{volume}% {icon}",
        "format-muted": "",
        "format-icons": {
            "headphone": "",
            "hands-free": "",
            "headset": "",
            "phone": "",
            "phone-muted": "",
            "portable": "",
            "car": "",
            "default": [
                "",
                ""
            ],
        },
        "scroll-step": 1,
        "on-click": "pavucontrol",
        "ignored-sinks": [
            "Easy Effects Sink"
        ]
    },
    "network": {
        "interface": "eno2",
        "format": "{ifname}",
        "format-wifi": " ",
    }
}