	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": { "fontStyle": "italic", "foreground": "#7a8478" }
		},
		{
			"name": "Variables",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": { "foreground": "#d3c6aa" }
		},
		{
			"name": "Keyword, Storage",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": { "foreground": "#e69875" }
		},
		{
			"name": "Operator, Misc",
			"scope": ["keyword.control", "punctuation", "meta.tag"],
			"settings": { "foreground": "#83c092" }
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag", "meta.tag.sgml"],
			"settings": { "foreground": "#e67e80" }
		},
		{
			"name": "Function, Special Method",
			"scope": ["entity.name.function", "meta.function-call", "variable.function"],
			"settings": { "foreground": "#7fbbb3" }
		},
		{
			"name": "Number, Constant",
			"scope": ["constant.numeric", "constant.language"],
			"settings": { "foreground": "#d699b6" }
		},
		{
			"name": "String, Symbols",
			"scope": ["string", "constant.other.symbol"],
			"settings": { "foreground": "#a7c080" }
		},
		{
			"name": "Class, Support",
			"scope": ["entity.name", "support.type", "support.class"],
			"settings": { "foreground": "#dbbc7f" }
		},
		{
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": { "foreground": "#e69875" }
		},
		{
			"name": "CSS Classes",
			"scope": ["entity.other.attribute-name.class"],
			"settings": { "foreground": "#dbbc7f" }
		},
		{
			"name": "JSON Key",
			"scope": ["support.type.property-name.json"],
			"settings": { "foreground": "#a7c080" }
		},
		{
			"name": "Markdown - Heading",
			"scope": ["markdown.heading", "markup.heading"],
			"settings": { "foreground": "#a7c080" }
		},
		{
			"name": "Markdown - Link",
			"scope": ["string.other.link.title.markdown"],
			"settings": { "foreground": "#7fbbb3" }
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": { "foreground": "#e67e80" }
		}
	]
}